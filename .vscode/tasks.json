{
	"version": "2.0.0",
	"tasks": [
		{
			"label": "Build",
			"type": "typescript",
			"tsconfig": "tsconfig.json",
			"problemMatcher": [
				"$tsc"
			],
			"group": {
				"kind": "build",
				"isDefault": true
			}
		},
		{
			"label": "Test",
			"type": "shell",
			"command": "bash -v test.sh",
			"group": {
				"kind": "test",
				"isDefault": true
			}
		}
	]
}